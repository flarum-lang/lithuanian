{
    "name": "flarum-lang/lithuanian",
    "description": "Lithuanian language extension pack for Flarum.",
    "type": "flarum-extension",
    "keywords": ["extension", "flarum", "lithuanian", "locale", "translation"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/flarum-lang/lithuanian/issues",
        "source": "https://github.com/flarum-lang/lithuanian"
    },
    "require": {
        "flarum/core": "^1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "flarum-extension": {
            "title": "Lithuanian",
            "category": "language",
            "info": {
                "website": "https://github.com/flarum-lang/lithuanian",
                "support": "https://github.com/flarum-lang/lithuanian/issues"
            },
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "lt",
            "title": "Lietuvi≈≥"
        }
    }
}
